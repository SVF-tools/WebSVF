{
    "name": "svftools",
    "displayName": "SVF ANALYSIS TOOLS",
    "description": "SVF ANALYSIS TOOLS",
    "icon": "img/icon.png",
    "version": "0.0.1",
    "publisher": "TianyangGuan",
    "author": {
        "name": "Tianyang Guan",
        "email": "spcidealacm@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spcidealacm/BugReport.git"
    },
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "AnalysisEnv",
                "title": "INSTALL SVF TOOLS ENV",
                "icon": {
                    "light": "$(extensions)",
                    "dark": "$(extensions)"
                }
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "svfbackend",
                    "name": "SVF BACK-END"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/jquery": "^3.5.1",
        "@types/jsdom": "^16.2.3",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.6.0",
        "@types/vscode": "^1.43.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {}
}
