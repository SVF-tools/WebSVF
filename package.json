{
	"name": "codemapplugin",
	"displayName": "CodeMap Plugin",
	"description": "Work in Progress",
	"version": "0.0.1",
	"publisher": "Zicheng",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.init",
				"title": "Report init"
			},
			{
				"command": "extension.stop",
				"title": "Report stop"
			},
			{
				"command": "extension.bugreport",
				"title": "Bug Report",
				"icon": {
					"light": "./public/images/bug.png",
					"dark": "./public/images/bug.png"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "isLinux || isMac || isWindows",
					"command": "extension.bugreport",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {}
}
